<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:CHAT.Resources.Styles">
    <Color x:Key="darkBlue">#33363d</Color>
    <SolidColorBrush Color="{StaticResource darkBlue}" x:Key="Main"></SolidColorBrush>

    <Color x:Key="CeladonBlue">#3e424a</Color>
    <SolidColorBrush Color="{StaticResource CeladonBlue}" x:Key="Light"></SolidColorBrush>
</ResourceDictionary>